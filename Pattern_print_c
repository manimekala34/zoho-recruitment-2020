// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n,i,j;
    scanf("%d",&n);
    //printf("%d",n);
    
    
    for(i=n;i>=1;i--) {
        for(j=n;j>=1;j--) {
           
            if(i>=j) {
                printf("%d",i);
            } else {
                printf("%d",j);
            }
            printf("\t");
        }
        
        printf("\n");
    }
    
    
    return 0;
}
